{
  "name": "git-watcher",
  "version": "1.0.0",
  "description": "super lightweight CI tool",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "start-dev": "nodemon --watch ./src src/index.ts ./test-config-file.json",
    "start-inspect": "nodemon --exec 'node --inspect -r ts-node/register' --watch ./src src/index.ts ./test-config-file.json",
    "start-mock": "ts-node ./src/mocks/index.ts",
    "prettier": "prettier --write .",
    "prettier-check": "prettier --check ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/isaac-j-miller/git-watcher.git"
  },
  "author": "Isaac Miller",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/isaac-j-miller/git-watcher/issues"
  },
  "homepage": "https://github.com/isaac-j-miller/git-watcher#readme",
  "dependencies": {
    "argparse": "^2.0.1",
    "axios": "^0.24.0",
    "express": "^4.17.2"
  },
  "devDependencies": {
    "@types/argparse": "^2.0.10",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "chalk": "^4.1.2",
    "eslint": "^8.7.0",
    "jest": "^27.4.7",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
}
